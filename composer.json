	"type": "composer-plugin",
	"licence": "MIT",
	"require": {
		"php": "^5.5.9|>=7.0.8",
		"composer-plugin-api": "^1.1",
		"symfony/console": "3.4.*"
	},
	"autoload": {
		"psr-4": {
			"Bayardev\\": "src/"
		}
	},
	"extra": {
		"class": "Bayardev\\Composer\\Plugin"
	},
	"require-dev": {
		"composer/composer": "^1.6"
	}
}
